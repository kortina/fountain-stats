{
    "runInTerminal.commands": [
        //////////////////////
        // jest typescript
        //////////////////////
        // jest: test line
        {
          "cmd": "./jest-focused.sh ${relativeFile}:${line}",
          "match": "\\.ts$",
          "name": "l"
        },
        // jest: test suite
        {
          "cmd": "npm run jest",
          "match": "\\.ts$",
          "name": "s"
        }
    ]
}